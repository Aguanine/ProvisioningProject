<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flat-profile xmlns="http://www.sipura.net/xsd/WRP400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sipura.net/xsd/WRP400 http://www.sipura.net/xsd/WRP400/WRP400-2-00-32.xsd">

  <Syslog_Server ua="na">10.0.0.1</Syslog_Server>
  <Debug_Server ua="na"></Debug_Server>
  <Debug_Level ua="na">0</Debug_Level> <!-- options: 0/1/2/3 -->

  <!-- Configuration Profile -->

  <Provision_Enable ua="na">Yes</Provision_Enable>
  <Resync_On_Reset ua="na">Yes</Resync_On_Reset>
  <Resync_Random_Delay ua="na">2</Resync_Random_Delay>
  <Resync_Periodic ua="na">10</Resync_Periodic>
  <Resync_Error_Retry_Delay ua="na">5</Resync_Error_Retry_Delay>
  <Forced_Resync_Delay ua="na">10</Forced_Resync_Delay>
  <Resync_After_Upgrade_Attempt ua="na">Yes</Resync_After_Upgrade_Attempt>
  <Resync_Fails_On_FNF ua="na">Yes</Resync_Fails_On_FNF>
  <Profile_Rule ua="na">/wrp400.cfg</Profile_Rule>
  <Profile_Rule_B ua="na"></Profile_Rule_B>
  <Profile_Rule_C ua="na"></Profile_Rule_C>
  <Profile_Rule_D ua="na"></Profile_Rule_D>
  <Log_Resync_Request_Msg ua="na">$PN -- $SWVER -- $MAU -- $SN -- Requesting resync $SCHEME://$SERVIP:$PORT$PATH</Log_Resync_Request_Msg>
  <Log_Resync_Success_Msg ua="na">$PN -- $SWVER -- $MAU -- $SN -- Successful resync $SCHEME://$SERVIP:$PORT$PATH</Log_Resync_Success_Msg>
  <Log_Resync_Failure_Msg ua="na">$PN -- $SWVER -- $MAU -- $SN -- Resync failed: $ERR</Log_Resync_Failure_Msg>

  <!-- Firmware Upgrade -->

  <Upgrade_Enable ua="na">Yes</Upgrade_Enable>
  <Upgrade_Error_Retry_Delay ua="na">5</Upgrade_Error_Retry_Delay>
  <Upgrade_Rule ua="na">/WRP400_2.00.32_000_100912_1440_ETSI.bin</Upgrade_Rule>
  <Log_Upgrade_Request_Msg ua="na">$PN -- $SWVER -- $MAU -- $SN -- Requesting upgrade $SCHEME://$SERVIP:$PORT$PATH</Log_Upgrade_Request_Msg>
  <Log_Upgrade_Success_Msg ua="na">$PN -- $SWVER -- $MAU -- $SN -- Successful upgrade $SCHEME://$SERVIP:$PORT$PATH -- $ERR</Log_Upgrade_Success_Msg>
  <Log_Upgrade_Failure_Msg ua="na">$PN -- $SWVER -- $MAU -- $SN -- Upgrade failed: $ERR</Log_Upgrade_Failure_Msg>

</flat-profile>
